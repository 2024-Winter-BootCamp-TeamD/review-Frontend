{
  "manifest_version": 3,
  "name": "My React Extension",
  "version": "1.0",
  "description": "A React extension built with Vite",
  "action": {
    "default_popup": "index.html",
    "default_icon": "src/assets/images/icon.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ]
}
